{
  "name": "aljud-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "npm run compile-admin-ui && concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "compile-admin-ui": "ts-node ./src/compile-admin-ui.ts"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "3.4.0",
    "@vendure/asset-server-plugin": "3.4.0",
    "@vendure/core": "3.4.0",
    "@vendure/dashboard": "^3.4.0",
    "@vendure/email-plugin": "3.4.0",
    "@vendure/graphiql-plugin": "3.4.0",
    "@vendure/ui-devkit": "^3.4.0",
    "dotenv": "17.2.1",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@vendure/cli": "3.4.0",
    "concurrently": "9.2.0",
    "typescript": "5.8.2"
  }
}
